         <span class="hljs-built_in">url</span>(<span class="hljs-string">'fonts/Sga-Regular.woff'</span>) <span class="hljs-built_in">format</span>(<span class="hljs-string">'woff'</span>),
         <span class="hljs-built_in">url</span>(<span class="hljs-string">'fonts/Sga-Regular.ttf'</span>) <span class="hljs-built_in">format</span>(<span class="hljs-string">'truetype'</span>);
