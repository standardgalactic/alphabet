    <span class="hljs-comment"># Read the image</span>
    img = cv2.imread(file_path, cv2.IMREAD_UNCHANGED)

    <span class="hljs-comment"># Check if image is in RGBA format (has an alpha channel)</span>
    <span class="hljs-keyword">if</span> img.shape[<span class="hljs-number">2</span>] == <span class="hljs-number">4</span>:
        <span class="hljs-comment"># Separate the color and alpha channels</span>
