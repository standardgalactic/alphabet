<span class="hljs-function"><span class="hljs-title">rotate_rot13</span></span>() {
    <span class="hljs-built_in">echo</span> <span class="hljs-string">"<span class="hljs-variable">$1</span>"</span> | <span class="hljs-built_in">tr</span> <span class="hljs-string">'A-Za-z'</span> <span class="hljs-string">'N-ZA-Mn-za-m'</span>
}

<span class="hljs-comment"># Replace 'Hello' with your desired word or phrase</span>
input=<span class="hljs-string">"Hello"</span>

rotated=$(rotate_rot13 <span class="hljs-string">"<span class="hljs-variable">$input</span>"</span>)

<span class="hljs-built_in">echo</span> <span class="hljs-string">"Original: <span class="hljs-variable">$input</span>"</span>
<span class="hljs-built_in">echo</span> <span class="hljs-string">"Rotated : <span class="hljs-variable">$rotated</span>"</span>
